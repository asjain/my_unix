filetype plugin indent on
syntax on
set backspace=2
set number
colorscheme desert
syntax on
"set gfn=Monospace\ 13
set guifont=DEC\ Terminal\ 14
filetype indent plugin on
set autowrite
set sw=4
set hlsearch
set ignorecase
set smartcase
set tabstop=4
set shiftwidth=4
set smarttab
set cursorline
au BufNewFile,BufRead *.svh set filetype=verilog_systemverilog
au BufNewFile,BufRead *.dv set filetype=make
#test
